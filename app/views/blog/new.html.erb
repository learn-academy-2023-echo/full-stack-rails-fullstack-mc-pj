<h3>Write a new Blog</h3>
<%= form_with model: @blog do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <br />
  <%= form.label :content %>
  <%= form.text_field :content %>
  <br /> 
  <%= form.submit %>
  <% end %>
  <br />

  <p>Back to <%= link_to 'Blog List', blogs_path %></p>